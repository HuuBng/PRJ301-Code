<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet1</servlet-name>
        <servlet-class>servlets.NewServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet2</servlet-name>
        <servlet-class>servlets.NewServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet4</servlet-name>
        <servlet-class>servlets.NewServlet4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet3</servlet-name>
        <servlet-class>servlets.NewServlet3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calculator2</servlet-name>
        <servlet-class>servlets.Calculator2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Equation2</servlet-name>
        <servlet-class>servlets.Equation2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet1</servlet-name>
        <url-pattern>/s1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet2</servlet-name>
        <url-pattern>/s2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet4</servlet-name>
        <url-pattern>/s4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet3</servlet-name>
        <url-pattern>/s3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calculator2</servlet-name>
        <url-pattern>/calculator2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Equation2</servlet-name>
        <url-pattern>/equation2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
